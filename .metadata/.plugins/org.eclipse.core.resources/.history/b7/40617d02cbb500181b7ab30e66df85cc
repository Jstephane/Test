
public class ExoConditionIf {

	public static void main(String[] args) {
		
		double maNote=15;
		int monCoef=2;
		double Resultat = maNote* monCoef;
		
		if (Resultat >= 50)   { 
			System.out.println("Resultat =" + Resultat);
		}
		
		if (Resultat < 50)   { 
			System.out.println("Resultat Insuffisant");

	   }
		
		double maNoteJava=10.5;
		double maNoteHTLM=13.5;
		double maNoteJavaSCRIPT=9.5;
		double maCSS=8.5;
		double maMoyenne = ( ( maNoteJava + maNoteHTLM + maNoteJavaSCRIPT + maCSS ) /4   );
		if (maMoyenne > 10)   { 
			System.out.println("Vous avez eu une bonne note");
		}
		if (maMoyenne < 10)   { 
			System.out.println("Vous avez eu une mauvaise note");
	}
}

}
	

    