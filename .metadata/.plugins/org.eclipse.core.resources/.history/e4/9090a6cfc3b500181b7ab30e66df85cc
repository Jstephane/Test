
public class ExoConditionIf {

	public static void main(String[] args) {
		
		int maNote=15;
		int monCoef=1;
		int Resultat = maNote* monCoef;
		
		if (Resultat >= 50)   { 
			System.out.println("Resultat =" + Resultat);
		}
		
		if (Resultat < 50)   { 
			System.out.println("Le Resultat n'est pas bon");

	   }
	}
}
