
public class ExoVariables {

	public static void main(String[] args) {
         
		
		
		double maNote=15;
		int monCoef=2;
	    double monResultat=( maNote * monCoef);
	    System.out.println("maNote = " + maNote);
	    System.out.println("monCoef = " + monCoef);
	    System.out.println("monResultat = " + monResultat);
	    
	    
	    maNote=34;
	    monCoef=6;
	    monResultat=( maNote * monCoef);
	    System.out.println("maNote = " + maNote);
	    System.out.println("monCoef = " + monCoef);
	    System.out.println("monResultat = " + monResultat);
	    
	    
	    maNote=15;
	    monCoef=2;
	    System.out.println("maNote = " + maNote);
	    System.out.println("monCoef = " + monCoef);
	    System.out.println("monResultat = " + monResultat);
	    
	    
	    maNote=15;
	    monCoef=2;
	    monResultat=( maNote * monCoef);
	    System.out.println("maNote = " + maNote);
	    System.out.println("monCoef = " + monCoef);
	    System.out.println("monResultat = " + monResultat);
	    
	    
	    
	    maNote=12.3;
	    monCoef=5;
	    monResultat=( maNote * monCoef);
	    System.out.println("maNote = " + maNote);
	    System.out.println("monCoef = " + monCoef);
	    System.out.println("monResultat = " + monResultat);
	    
	    
	    
	    double maNoteJava = 10.5;
		double maNoteHTML = 13.2;
		double maNoteJAVAScript = 11.3;
		double maNoteCSS =8.5;
	    double maMoyenne=( (maNoteJava + maNoteHTML + maNoteJAVAScript + maNoteCSS)/4 );
	    System.out.println("maNoteJava = " + maNoteJava);
	    System.out.println("maNoteHTML = " + maNoteHTML);
	    System.out.println("maNoteJAVAScript = " + maNoteJAVAScript);
		System.out.println("maNoteCSS = " + maNoteCSS);
		System.out.println("maMoyenne = " + maMoyenne);
	    		
	    int monCoefJAVA = 3;
	    int monCoefHTML= 2;
	    int monCoefJAVAScript = 3;
	    int monCoefCSS = 1;
	    int TotalCoef =(monCoefJAVA + monCoefHTML + monCoefJAVAScript + monCoefCSS);
	    double maMoyenneavecCoef=( (maNoteJava*monCoefJAVA + maNoteHTML*monCoefHTML + maNoteJAVAScript*monCoefJAVAScript + maNoteCSS*monCoefCSS)/ TotalCoef );
	    System.out.println("monCoefJAVA = " + monCoefJAVA);
	    System.out.println("monCoefHTML = " + monCoefHTML);
	    System.out.println("monCoefNoteJAVAScript = " + monCoefJAVAScript);
		System.out.println("monCoefNoteCSS = " + monCoefCSS);
		System.out.println("maMoyenne = " + maMoyenneavecCoef);
		
		
	
		
		System.out.println(" x" );
		System.out.println(" xx" );
		System.out.println("xxx" );
		System.out.println("xxxx" );
		
		
		
		
		
		
		
	    
	    		
	    	
	
	
	

}
}