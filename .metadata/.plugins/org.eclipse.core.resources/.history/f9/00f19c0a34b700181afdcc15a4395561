
public class WhileBoucle {

	public static void main(String[] args) {
	
		 

		int VariableX=1;
		int VariableY=100;
		
		while (VariableX <=101 ) {
	
			VariableX =VariableX + 1 ;

	    while (VariableY>= 1 ) {
	
				VariableY--;	
				
				System.out.println("VariableX = " + VariableX       +     "  VariableY = " + VariableY);
				
				}
				
	     
		}
	}
	
}


