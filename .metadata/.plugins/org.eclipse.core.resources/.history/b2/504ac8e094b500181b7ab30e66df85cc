
public class Typesvariables {

	public static void main(String[] args) {
    int entier=14;
    System.out.println("La variable entier = " + entier);
    float decimal=12.5f;
    System.out.println("La variable decimale = " + decimal);
    double decimaldoubleprec =15.5;
    System.out.println("La variable decimaldoubleprec = " + decimaldoubleprec);
    Boolean bool = true;
    System.out.println("La variable bool = " + bool);
    char caractere ='z';
    System.out.println("La variabl"
    		+ ""
    		+ "e caractere = " + caractere);
    String chaine ="Hello";
    System.out.println("La variable String = " + chaine);
    System.out.println();
   
    System.out.println("La variable entier1 * La variable entier2 = " + (12*12));
    int VariableEntier1 = 12;
    int VariableEntier2 = 4;
    System.out.println("La variable entier1 * La variable entier2 = " + (VariableEntier1*VariableEntier2));
    

    int Prix =10;
    int TVA=20;
    System.out.println("Prix * TVA = " + (Prix * TVA ));
    
 
    int VariablePrix =25;
    int VariableTVA=35;
    System.out.println("La Variable Prix * La Variable TVA = " + (VariablePrix * VariableTVA ));
    System.out.println();

    Prix=34;
    System.out.println("Prix * TVA = " + (Prix * TVA ));
    TVA=21;
    System.out.println("Prix * TVA = " + (Prix * TVA ));
    
    
   double TVAdouble=19.6;
   System.out.println("Prix * TVAdouble = " + (Prix * TVAdouble ));
   
	}

}
